<!DOCTYPE html>
<html>
<head>
  <title>Achs</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag href = 'https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i' %>

</head>

<body id="page-top">
<div id="wrapper">
  <%= render 'layouts/navigation/sidebar' %>
  <div id="content-wrapper" class="d-flex flex-column">
    <div id="content">
      <%-# <%= render 'layouts/navigation/header' -%>
      <div class="container-fluid">
        <%= yield %>
      </div>
    </div>
    <%= render 'layouts/navigation/footer' %>

  </div>
</div>
<% if flash[:error] %>
  <div class="flash"><%= flash[:error] %></div>
<% end %>
</body>
</html>
